{
  "author": "cnjimbo <cnjimbo@dmsrs.org>",
  "dependencies": {
    "reactive-vscode": "^0.2.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.1",
    "@antfu/ni": "^0.22.4",
    "@types/eslint": "^9.6.1",
    "@types/vscode": "^1.92.0",
    "eslint": "^9.9.1",
    "eslint-factory": "^0.0.2",
    "ofetch": "^1.3.4",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "funding": "https://github.com/sponsors/cnjimbo",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ext:build": "cd extension && tsup src/index.ts --external vscode",
    "ext:dev": "nr ext:build --watch src",
    "ext:pack": "nr ext:build && cd extension && vsce package --no-dependencies",
    "ext:publish": "nr ext:build && cd extension && vsce publish --no-dependencies",
    "lint": "eslint .",
    "update": "npx vscode-ext-gen ./extension/package.json --readme ./extension/README.md --output extension/src/generated-meta.ts"
  },
  "type": "module",
  "workspaces": [
    "extension"
  ]
}
