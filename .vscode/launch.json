{
  "version": "0.2.0",
  "console": "integratedTerminal",
  "configurations": [
    {
      "name": "Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/extension/"
      ],
      "outFiles": [
        "${workspaceFolder}/extension/dist/**/*.cjs"
      ],
      
      "preLaunchTask": "npm: ext:dev",
      "sourceMaps": true
    }
  ]
}