{
  "activationEvents": [
    "onStartupFinished"
  ],
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "category": "Project Config Updater",
        "command": "project-config.manualUpdate",
        "title": "Update config now"
      }
    ],
    "configuration": {
      "properties": {
  
        "project-config.autoUpdate": {
          "default": true,
          "description": "Fetch and update the latest config automatically",
          "type": "boolean"
        },
        "project-config.annotations": {
          "default": true,
          "description": "Enabled project-config inline annotations",
          "type": "boolean"
        },
        "project-config.autoUpdateInterval": {
          "default": 4320,
          "description": "The minimal interval for auto update, in minutes",
          "type": "number"
        },
        "project-config.color": {
          "default": "auto",
          "description": "Icon color hex for inline displaying",
          "type": "string"
        },
        "project-config.position": {
          "default": "before",
          "description": "Position the icon before or after the icon name",
          "enum": [
            "after",
            "before"
          ],
          "type": "string"
        }
      },
      "title": "Project Config Updater",
      "type": "object"
    }
  },
  "description": "Export current settings to workspace config file ",
  "displayName": "Project Config Updater",
  "icon": "res/logo.png",
  "license": "MIT",
  "main": "./dist/index.js",
  "name": "project-config",
  "preview": true,
  "private": true,
  "publisher": "cnjimbo",
  "repository": {
    "directory": "extension",
    "type": "git",
    "url": "https://github.com/open-dmsrs/vscode-project-config-updater"
  },
  "version": "1.1.2"
}